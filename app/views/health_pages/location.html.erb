<div class="search">
<h2>Services Search</h2>
<%= form_tag(location_path, method: "get") do %>
  <%= label_tag(:location, "Enter Address: ") %><%= text_field_tag(:location) %>
  <br>
  <%= submit_tag "Search" %>
<% end %>

<div class="results animated fadeInUp">

<%= render "locationresults" %>

</div>
</div>
